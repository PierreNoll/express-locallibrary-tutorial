<div id="book_list">
  <div class="container">
    <h1><%- title%></h1>

    <ul>
      <% if (book_list.length){%>
      <% for (var i = 0; i < book_list.length; i++) {%>
        <li>
          <a href='<%=book_list[i].url%>'> <%- book_list[i].title%></a>
          <%- book_list[i].author.name%>
        </li>

        <% }} else {%>
          <li> There are no books.</li>
          <%}%>
        </ul>
      </div>
    </div>
